<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axel or.com/xml/ns/object-views/object-views_5.2.xsd">

<!-- Address related field -->
  <selection name="gst.address.type.select">
    <option value="default">default</option>
    <option value="invoice">invoice</option>
    <option value="shipping">shipping</option>
    <option value="other">other</option>
  </selection>


<!--   Party Related Field -->

  <selection name="gst.party.type.select">
    <option value="Company">Company</option>
    <option value="Individual">Individual</option>
  </selection>


<!--   contact related field -->

  <selection name="gst.contact.type.select">
      <option value="Primary">Primary</option>
        <option value="Secondary">Secondary</option>
        <option value="Others">Others</option>
    </selection>

<!-- bank account detail type -->
    <selection name="gst.bankaccount.type.select">
      <option value="current">current</option>
        <option value="saving">saving</option>
    </selection>
</object-views>
