<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axel or.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid name="gst-productcategory-grid" title="Product Categorie List"   model="com.axelor.gst.db.Productcategory">
    <field name="name"></field>
    <field name="code"></field>
    <field name="gstrate"></field>
  </grid>

  <form name="gst-productcategory-form" title="Product Categorie list"   model="com.axelor.gst.db.Productcategory">
    <panel title="Overview">
      <field name="name"></field>
      <field name="code"></field>
      <field name="gstrate" colSpan="12"></field>
    </panel>
  </form>
</object-views>
