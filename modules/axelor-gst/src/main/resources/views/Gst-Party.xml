<?xml version="1.0" encoding="UTF-8"?>

<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axel or.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid name="gst-party-grid" title="Party List" model="com.axelor.gst.db.Party">
     <field name="name"/>
      <field name="type"/>
      <field name="customer"/>
      <field name="supplier"/>
      <field name="gstin"/>
      <field name="contact"/>
      <field name="address"/>
  </grid>

  <form name="gst-party-form" title="Party"   model="com.axelor.gst.db.Party">
    <panel title="Overview">
      <field name="name"/>
      <field name="type"/>
      <field name="customer"/>
      <field name="supplier"/>
      <field name="gstin" colSpan="12"/>
      <field name="contact" widget="editor" colSpan="12"/>
      <field name="address" widget="editor" colSpan="12"/>

    </panel>
  </form>

</object-views>
