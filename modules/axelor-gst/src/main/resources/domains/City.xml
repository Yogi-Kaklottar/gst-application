<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="gst" package="com.axelor.gst.db" />

  <entity name="City" cacheable="true">
    <string name="name" unique="true" required="true"/>
    <many-to-one name="state" ref="com.axelor.gst.db.State"   required="true"/>
    <many-to-one name="country" ref="com.axelor.gst.db.Country" required="true" />
  </entity>

</domain-models>
